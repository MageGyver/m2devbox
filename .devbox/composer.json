{
    "name": "aeno/mage2-devbox",
    "description": "Manage multiple Magento 2 instances via Docker",
    "type": "project",
    "bin": ["devbox"],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4||^8.0",
        "ext-json": "*",
        "ext-posix": "*",
        "symfony/console": "^5.2",
        "vlucas/phpdotenv": "^5.2",
        "symfony/process": "^5.2",
        "alecrabbit/php-cli-snake": "^0.6",
        "symfony/filesystem": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {"Devbox\\": "src/"}
    },
    "repositories": {
        "alecrabbit/php-cli-snake": {
            "type": "vcs",
            "url": "https://github.com/aeno/php-cli-snake"
        }
    },
    "authors": [
        {
            "name": "Steffen Rieke",
            "email": "devbox@aenogym.de"
        }
    ],
    "config": {
        "sort-packages": true
    }
}
